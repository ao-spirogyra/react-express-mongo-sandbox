{
  "name": "react-express-mongo-sandbox",
  "version": "1.0.0",
  "engines": {
    "node": "14.16.1"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:client:production && npm run build:server:production",
    "build:client:development": "webpack",
    "build:client:production": "webpack --mode=production",
    "build:server:production": "tsc server/index.ts || exit 0",
    "start:development": "npm-run-all start:client start:server:development",
    "start:client:development": "webpack serve",
    "start:server:development": "nodemon -L",
    "start:server:production": "node server/index.js",
    "ts-node": "ts-node"
  },
  "author": "ao-spirogyra",
  "dependencies": {
    "@types/mongoose": "^5.10.5",
    "express": "^4.17.1",
    "mongoose": "^5.12.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
